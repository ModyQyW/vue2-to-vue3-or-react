{
  "name": "vue2-to-vue3-or-react",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "commit": "cz",
    "dev": "slidev --open",
    "export": "slidev export",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint && pnpm run lint:stylelint",
    "lint:eslint": "eslint . --fix --ext=.js,.jsx,.ts,.tsx,.vue --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@slidev/cli": "~0.27.18",
    "@slidev/theme-default": "~0.21.1",
    "@slidev/theme-seriph": "~0.21.1"
  },
  "devDependencies": {
    "@babel/core": "~7.16.0",
    "@babel/eslint-parser": "~7.16.3",
    "@commitlint/cli": "~15.0.0",
    "@modyqyw/fabric": "~4.2.1",
    "@typescript-eslint/eslint-plugin": "~5.6.0",
    "@typescript-eslint/parser": "~5.6.0",
    "eslint": "~8.4.1",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "lint-staged": "~12.1.2",
    "markdownlint-cli": "~0.30.0",
    "prettier": "~2.5.1",
    "release-it": "~14.11.8",
    "stylelint": "~14.1.0",
    "typescript": "~4.5.3"
  }
}
