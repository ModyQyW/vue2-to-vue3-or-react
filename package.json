{
  "name": "vue2-to-vue3-or-react",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "commit": "cz",
    "dev": "slidev --open",
    "export": "slidev export",
    "lint": "pnpm run lint:markdownlint",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@slidev/cli": "~0.27.18",
    "@slidev/theme-default": "~0.21.1",
    "@slidev/theme-seriph": "~0.21.1",
    "playwright-chromium": "~1.17.1"
  },
  "devDependencies": {
    "@commitlint/cli": "~15.0.0",
    "@modyqyw/fabric": "~4.2.1",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "lint-staged": "~12.1.2",
    "markdownlint-cli": "~0.30.0",
    "prettier": "~2.5.0",
    "release-it": "~14.11.8",
    "typescript": "~4.5.3"
  }
}
